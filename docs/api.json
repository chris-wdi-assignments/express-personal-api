{
  "message": "Here's my personal API.",
  "documentationUrl": "https://github.com/chris-wdi-assignments/express-personal-api/README.md",
  "baseURL": "https://stormy-lake-40912.herokuapp.com/",
  "endpoints": [
    {
      "method": "GET", "path": "/api", "description": "Describes all available endpoints."
    },
    {
      "method": "GET", "path": "/api/profile", "description": "Describes the developer"
    },
    {
      "method": "GET", "path": "/api/records", "description": "Lists my record collection."
    },
    {
      "method": "GET", "path": "/api/records/:id", "description": "Info about a specific record."
    },
    {
      "method": "POST", "path": "/api/records", "description": "Create a new album"
    },
    {
      "method": "PUT", "path": "/api/records/:id", "description": "Modify an existing album."
    },
    {
      "method": "DELETE", "path": "/api/records/:id", "description": "Destroy an album."
    }
  ]
}
